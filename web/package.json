{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && run-s pre-build post-build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "pre-build": "shx rm -rf ../public/themes/default",
    "post-build": "shx mv dist/index.html ../resources/views/themes/default/index.blade.php && shx mv dist/config.json ../resources/views/themes/default/config.json && shx cp -r dist/* ../public/ && shx rm -rf dist",
    "openapi-ts": "openapi-ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "@vicons/antd": "^0.12.0",
    "axios": "^1.8.4",
    "copy-image-clipboard": "^2.1.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "photoswipe": "^5.4.4",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.4.21",
    "vue-advanced-cropper": "^2.8.9",
    "vue-i18n": "11.1.2",
    "vue-next-masonry": "^1.1.3",
    "vue-photo-album": "^1.2.4",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@hey-api/client-axios": "^0.8.0",
    "@hey-api/openapi-ts": "^0.69.2",
    "@tailwindcss/typography": "^0.5.14",
    "@tsconfig/node20": "^20.1.4",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.12.5",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "naive-ui": "^2.38.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^5.14.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.3",
    "terser": "^5.39.0",
    "typescript": "~5.4.0",
    "vfonts": "^0.0.3",
    "vite": "^6.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-tsc": "^2.0.11"
  }
}

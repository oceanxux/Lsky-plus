{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "composer": "*",
        "alibabacloud/green": "^1.8",
        "alibabacloud/green-20220302": "^2.2",
        "alphasnow/aliyun-oss-laravel": "^4.8",
        "awcodes/filament-tiptap-editor": "^3.0",
        "aws/aws-sdk-php": "^3.307",
        "barryvdh/laravel-debugbar": "^3.13",
        "doctrine/dbal": "^4.2",
        "endroid/qr-code": "^5.0",
        "filament/filament": "^3.3",
        "flowframe/laravel-trend": "^0.2.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "intervention/image-driver-vips": "^1.0",
        "intervention/image-laravel": "^1.5",
        "laravel/fortify": "^1.21",
        "laravel/framework": "^11.0",
        "laravel/octane": "^2.9",
        "laravel/prompts": "^0.1.24",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "league/flysystem-webdav": "^3.0",
        "league/glide": "^3.0.0-beta",
        "mews/captcha": "^3.4",
        "overtrue/laravel-easy-sms": "^2.2",
        "overtrue/laravel-filesystem-cos": "^3.5",
        "overtrue/laravel-filesystem-qiniu": "^2.3",
        "overtrue/laravel-socialite": "^4.1",
        "propaganistas/laravel-phone": "^5.3",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-settings": "^3.4",
        "srmklive/paypal": "^3.0",
        "symfony/amazon-mailer": "^7.2",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/postmark-mailer": "^7.0",
        "tencentcloud/ims": "^3.0",
        "wispx/flysystem-upyun": "^1.0",
        "yansongda/pay": "~3.7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "laravel-lang/lang": "^15.4",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan lang:update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
